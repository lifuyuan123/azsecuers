<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:orientation="vertical"
android:layout_width="match_parent"
android:padding="5dp"
android:layout_height="50dp">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="系统进程"
        android:textColor="#dd5511"
        android:textSize="16dp"
        android:layout_marginRight="10dp"
        android:id="@+id/tv_phonequick_liststyle_right"
        android:layout_alignParentRight="true"
        android:layout_alignBottom="@+id/tv_phonequick_liststyle_buttom"
        />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="New Text"
    android:layout_marginLeft="10dp"
    android:id="@+id/tv_phonequick_liststyle_top"
    android:layout_alignTop="@+id/im_phone_quick_liststyle"
    android:layout_toRightOf="@id/im_phone_quick_liststyle"
    />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="New Text"
    android:layout_marginLeft="10dp"
    android:id="@+id/tv_phonequick_liststyle_buttom"
    android:layout_alignBottom="@+id/im_phone_quick_liststyle"
    android:layout_toRightOf="@id/im_phone_quick_liststyle"
    />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/im_phone_quick_liststyle"
    android:layout_toRightOf="@+id/phone_qiuck_liststyle_check"
    android:src="@drawable/ic_launcher"
    />

<CheckBox
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/phone_qiuck_liststyle_check"
    android:layout_centerVertical="true"
    />
</RelativeLayout>